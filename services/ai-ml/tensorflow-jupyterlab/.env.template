# TensorFlow Jupyter Lab Configuration

# Version
# Image version/tag to use
TENSORFLOW_VERSION=latest

# TensorFlow Base Image Tag
# Use 'latest' for CPU, 'latest-gpu' for GPU support
TENSORFLOW_TAG=latest

# Port Configuration
# Port for Jupyter Lab web interface (using 8889 to avoid conflict with other Jupyter services)
TENSORFLOW_JUPYTER_PORT=8889

# Jupyter Lab Configuration
# Enable Jupyter Lab interface (yes/no)
JUPYTER_ENABLE_LAB=yes
