# pgAdmin Configuration

# Version
PGADMIN_VERSION=latest

# Port
PGADMIN_PORT=8082

# Authentication
# IMPORTANT: Change these before deploying
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=YourStrong@Password

# Security
PGADMIN_COOKIE_PROTECTION=True
PGADMIN_LOGIN_BANNER="Authorised users only!"

# Logging
# 0=NOTSET, 10=DEBUG, 20=INFO, 30=WARNING, 40=ERROR, 50=CRITICAL
PGADMIN_LOG_LEVEL=10
